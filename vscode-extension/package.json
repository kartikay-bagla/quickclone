{
	"name": "oneclickgitclone",
	"displayName": "One Click Git Clone",
	"description": "Clone and open github repositories in one click.",
	"version": "0.0.1",
	"publisher": "ConfirmedVellas",
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.59.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"contributes": {
		"configuration":[
			{
				"title": "One Click Git Clone",
				"properties": {
					"oneclickgitclone.defaultFolder": {
						"type": "string",
						"default": "",
						"description": "Location of default folder to clone into"
					},
					"oneclickgitclone.alwaysOpen": {
						"type": "boolean",
						"default": false,
						"description": "Always open the cloned repository directly"
					}
				}
			}
		]
	}
}
